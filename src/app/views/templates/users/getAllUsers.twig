{% extends 'templates/base.twig' %}

{% block content %}
    <div class="row">
        <div class="col-md-12">
            <h1>Users</h1>
            <hr />
            <ul>
                {% for user in users %}
                    <li>
                        {{ user.username }}
                        <ul>
                            <li>{{ user.id }}</li>
                            <li>{{ user.email }}</li>
                        </ul>
                    </li>
                {% endfor %}
            </ul>
        </div>
    </div>
{% endblock %}